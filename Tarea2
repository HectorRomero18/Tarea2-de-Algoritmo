Funcion ejercicio25
	//Ejercicio N°25
	//Definimos estatura cant como entero
	//Definimos prom como real
	//Dimension cant[x] 
	//Pedimos al usuario que ingrese la cantidad de personas encuestadas
	//Leer cant[x]
	//Usamos el Mientras 
	//Calcylamos el promedio
	//Mostramos los resultados
	Definir estatura, cant Como Entero
	Definir prom Como Real
	Dimension cant[x]
	Escribir "Ingrese la cantidad de personas encuestadas"
	leer cant[x] 
	Mientras cant[x] > 1 Hacer
		Escribir "Ingrese la estatura de la persona"
		leer estatura
		cant[x] = cant[x] - 1
	Fin Mientras
	prom = estatura / cant[x]
	Escribir "El promedio de las estaturas es de:" , prom
FinFuncion

Funcion ejercicio26
	//Ejercicio N°26
	//Definimos x como entero
	//x = 0
	//Usamos el Mientras 
	//Usamos el Si-Entonces
	//Usamos el mod
	//Mostramos los resultados
	Definir x Como Entero
	x = 0
	Mientras x <= 100 Hacer
		si x mod 2 = 0 Entonces
			Escribir x
		FinSi
		x = x + 1
	Fin Mientras
FinFuncion

Funcion ejercicio27
	//Ejercicio N°27
	//Definimos x, z , total como entero
	//x = 0
	//Usamos el Mientras 
	//Mostramos los resultados
	Definir x, z, total Como Entero
	x = 0
	Mientras x <= 10 Hacer
		Escribir "Ingrese un numero"
		leer z
		x = x + 1
	Fin Mientras
	total = z + z
	Escribir "La suma de los numeros ingresados es de:" , total 
FinFuncion

Funcion ejercicio28
	//Ejercicio N°28
	//Definimos cantidad, edad como entero
	//Definimos prom como real
	//Pedimos al usuari que in grese la cantidad de alumnos encuestados
	//Leer cantidad
	//Usamos el Mientras 
	//Calculamos el promedio
	//Mostramos los resultados
	Definir cantidad, edad  Como Entero
	Definir prom Como Real
	Escribir "Ingrese la cantidad de alumnos encuestados"
	leer cantidad 
	Mientras cantidad > 1 Hacer
		Escribir "Ingrese la edad de los alumnos"
		leer edad 
		cantidad = cantidad - 1
	Fin Mientras
	prom = edad / cantidad 
	Escribir "El promedio de las edades de los alumnos es de:" , prom 
FinFuncion

Funcion ejercicio29
	//Ejercicio N°29
	//Definimos registroh, valorh, totalh, sueldoh como entero
	//Pedimos al usuario que ingrese la cantidad de horas que trabaja
	//Leer registroh
	//Pedimos al usuario que ingrese el valor a pagar por cada hora de trabajo
	//Leer valorh
	//totalh = registroh * 20
	//sueldoh = valorh * totalh 
	//Mostramos los resultados
	Definir registroh, valorh, totalh, sueldoh Como Entero
	Escribir "Ingrese la cantidad de horas que trabajo en los ultimos 20 dias"
	leer registroh
	Escribir "Ingrese el valor a pagar por cada hora de trabajo"
	leer valorh
	totalh = registroh * 20
	Escribir "El total de las horas trabajadas es de:" , totalh 
	sueldoh = valorh * totalh 
	Escribir "Su sueldo por horas trabajadas es de:" , sueldoh "$"
FinFuncion

Funcion ejercicio30
	//Ejercicio N°30
	//Definimos n, i, c, x como entero
	//Dimension vector[n]
	Definir n, i, c, x Como Entero
	Escribir "Ingrese la cantidad de ventas al dia"
	leer n
	Dimension vector[n] 
	Mientras vector[n] > 1 Hacer
		Escribir "Ingrese el valor de los productos que vendio"
		leer x
		vector[n] = vector[n] - 1
	Fin Mientras
	Escribir "Ingrese el valor que quieres buscar"
	Escribir "1: Mayores 1000$"
	Escribir "2: Mayores a 500$ o iguales a 1000$"
	Escribir "3: Menores a 500$"
	leer x
	c = 0
	Para i = 0 Hasta n - 1 Con Paso 1 Hacer
		si x = vector[i] Entonces
			c = c + 1
		FinSi
	Fin Para
	Escribir "Los productos con un valor de" , x , "se vendieron un total de:", c " unidades" 
FinFuncion

Funcion ejercicio31
	//Ejercicio N°31
	//Definimos x,c,a,b como entero
	//Definimos prom como real
	//Pedimos al usuario que ingrese la cantidad de alumnos a promediar
	//Leer x
	//Usamos el Mientras 
	//Calculamos promedios
	//Mostramos los resultados
	Definir x,c,a,b Como Entero
	Definir prom Como Real
	Escribir "Ingrese la cantidad de alumnos a promediar"
	leer x
	Mientras x > 1 Hacer
		Escribir "Ingrese la calificacion de la asignatura de logica"
		leer c
		x = x -1
	Fin Mientras
	Mientras x > 1 Hacer
		Escribir "Ingrese la calificacion de la asignatura de requerimientos"
		leer a
		x = x - 1
	Fin Mientras
	Mientras x > 1 Hacer
		Escribir "Ingrese la calificacion de la asignatura de calculo"
		leer b
		x = x - 1
	Fin Mientras
	prom = c + a + b / x
	Escribir "El promedio de los alumnos en todas las materias es de:", prom
FinFuncion

Funcion ejercicio32
	//Ejercicio N°32
	//Definimos categoria como Caracter 
	//Definimos prom, sueldo, total, calculo como real
	//Pedimos qe ingrese su sueldo
	//Leer sueldo
	//Pedimos que ingrese su categoria
	//Leer categoria
	//Usamos el Segun 
	//Mostramos los resultados
	Definir categoria Como Caracter
	Definir prom, sueldo, calculo, total Como Real
	Escribir "Ingrese su sueldo"
	leer sueldo
	Escribir "Ingrese la categoria"
	leer categoria
	Segun categoria Hacer
		'Auxiliar':
			Escribir "Tienes un incremento del 10%"
			calculo = sueldo * 10 / 100
			total = sueldo + calculo
			Escribir "Su nuevo sueldo es de:" , total "$"
			
		'Agregado':
			Escribir "Tienes un incremento del 20%"
			calculo = sueldo * 20 / 100
			total = sueldo + calculo
			Escribir "Su nuevo sueldo es de:" , total "$"
		'Principal':
			Escribir "Tienes un incremento del 30%"
			calculo = sueldo * 30 / 100
			total = sueldo + calculo
			Escribir "Su nuevo sueldo es de:" , total "$"
		De Otro Modo:
			Escribir "Categoria inexistente"
	Fin Segun
FinFuncion

Funcion ejercicio33
	//Ejercicio N°33
	//Definimos antidad, cantb, pasaje, recorrido, total Como entero
	//Pedimos al usuario que ingrese la cantidad de viajes
	//Leer cantidad
	//Pedimos al usuario que ingrese cuanto va a ser el recorrido
	//Leer recorrido
	//Pedimos al usuario que ingrese el costo del pasaje
	//Leer pasaje
	//Usamos el Si-Entonces
	//Mostramos los resultados
	Definir cantidad, cantb, pasaje, recorrido, total Como Entero
	Definir prom como real
	Escribir "Ingrese la cantidad de viajes"
	leer cantidad
	Escribir "De cuanto va a ser el recorrido ?"
	leer recorrido
	Escribir "Ingrese el costo del pasaje"
	leer pasaje
	Si recorrido <= 100 Entonces
		Escribir "No tiene incremento"
		total = pasaje
	Fin Si
	si recorrido > 100 Entonces
		Escribir "Tiene un incremento del 20%"
		calculo = pasaje * 20 / 100
		total = pasaje + calculo
	FinSi
	Escribir "El precio final es de:" , total "$"
FinFuncion

Funcion ejercicio34
	//Ejercicio N°34
	//Definimos a, b como entero
	//Definimos x, suma1, prom como real
	//x = 1
	//a = 0
	//b = 0
	//Usamos el Mientras 
	//Mostramos los resultados
	Definir a, b Como Entero
	Definir x,suma1, prom Como Real
	x = 1
	a = 0
	b = 0
	Mientras x <> 0 Hacer
		Escribir "Ingrese un numero"
		leer x
		si x <> 0 Entonces
			a = a + 1
			si x > 0 Entonces
				b = b + 1
				suma1 = suma1 + x
				prom = suma1 / a
			FinSi
		FinSi
	Fin Mientras
	Escribir "El total de los numero es:", a
	Escribir "El promedio de los numeros es de:", prom
FinFuncion

Funcion ejercicio35
	//Ejercicio N°35
	//Definimos num, x como entero
	//Pedimos al usuario que ingrese la cantidad de numeros que desea ingresar
	//Leer x
	//Usamos el Mientras 
	//Usamos el Si-Entonces
	//Mostramos los resultados
	Definir num, x Como Entero
	Escribir "Ingrese la cantidad de numeros que desea ingresar"
	leer x
	Mientras x >= 0 Hacer
		Escribir "Imgrese un numero"
		leer num 
		x = x - 1
	Fin Mientras
	total = num + num
	Si num mod 3 = 0 Entonces
		Escribir "La cantidad de numeros multiplos de 3 son:", num
	Fin Si
	Escribir "La suma de los numeros ingresados es de:" , total
FinFuncion
Algoritmo Ejercicios_Pseint
	definir ejerc como entero
	Escribir "Ingrese el numero del ejercicio que quieres ejecutar(1-35)"
	leer ejerc 
	Esperar Tecla
	Esperar 1 Segundos
	Segun ejerc Hacer
		1:
			Limpiar Pantalla
			Escribir "Leer un Caracter"
			ejercicio1
		2:
			Limpiar Pantalla
			Escribir "Deducir si es un numero o vocal"
			ejercicio2
		3:
