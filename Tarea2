FinFuncion

Funcion ejercicio23
	//Ejercicio N°23
	//Definimos dia, dias como Caracter
	//Definimos num, pos como entero
	//Dimension [dia]
	//Pedimos al usuario que ingres el numero del dia
	//Leer num 
	//Usamos el Si-Entonces
	//Mostramos los resultados
	Definir dia, dias Como Caracter
	Definir num, pos como entero
	Dimension dia[7]
	dia[1] = "Lunes"
	dia[2] = "Martes"
	dia[3] = "Miercoles"
	dia[4] = "Jueves"
	dia[5] = "Viernes"
	dia[6] = "Sabado"
	dia[7] = "Domingo"
	Escribir "Ingrese el numero del dia"
	leer num 
	Si (num1 >= 1) y (num <= 7) Entonces
		dias = dia[num]
		Escribir "Este numero corresponde al dia:", dias 
	SiNo
		Escribir "ERROR"
	Fin Si
	
FinFuncion

